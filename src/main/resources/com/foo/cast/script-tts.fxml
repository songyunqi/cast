<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.foo.cast.ScriptTTSController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label fx:id="scriptFilePath" text="xlsx文件路径"/>
    <Button fx:id="fileChoose" text="选择..." onAction="#onFileChooseClick"/>
    <Button fx:id="tts" text="TTS生成" onAction="#onTTSClick"/>

    <Button fx:id="ttsCombine" text="Wav合并" onAction="#onTTSCombineClick"/>
</VBox>
